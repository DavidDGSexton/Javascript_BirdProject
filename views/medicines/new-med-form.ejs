<!--This will be the form that the user will use to add a new medicine to the database-->
<!DOCTYPE html>
<html>
  <%- include('../partials/header', {title: 'medicine Project'}); %>
  <body>
    <% include ../partials/navbar %>

    <div class="container">
      <h1>New medicine!</h1>
      <!--This is a form template from boostrap that I put in here by following along in class on 10/14/2020-->
      <form method="POST" action="/medicines/create-medicine" enctype="text/html">
        <div class="form-group col-4">
          <label for="medicineName">Name</label>
          <input type="text" class="form-control" id="medicineName" name="name" placeholder="Enter Name">
        </div>
        <button type="submit" value="Create" class="btn btn-primary" style="margin-top: 10px;">Submit</button>
      </form>
      <!--End Boostrap Form-->
    </div>
    <% include ../partials/scripts %>
  </body>
</html>
